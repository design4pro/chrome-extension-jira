{
    "name": "chrome-extension-jira",
    "version": "1.0.0",
    "description": "Chrome Extension for JIRA",
    "main": "index.js",
    "homepage": "https://github.com/design4pro/chrome-extension-jira",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --progress --colors --hide-modules",
        "build:dev": "cross-env NODE_ENV=development webpack --progress --colors --hide-modules",
        "build:zip": "node scripts/build-zip.js",
        "watch": "npm run build -- --watch",
        "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
        "jsdoc": "jsdoc src -r",
        "deploy_jsdoc": "yarn jsdoc &&  gh-pages -d out",
        "prettier": "prettier \"src/**/*.{js,jsx}\"",
        "prettier:write": "npm run prettier -- --write",
        "lint": "eslint --ext .js,.jsx src",
        "test": "yarn prettier_check_src && yarn prettier_check_popup && yarn prettier_check_option && yarn prettier_check_content && yarn build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "7.7.2",
        "@babel/plugin-proposal-class-properties": "7.7.0",
        "@babel/plugin-syntax-class-properties": "7.2.0",
        "@babel/plugin-syntax-dynamic-import": "7.2.0",
        "@babel/plugin-transform-runtime": "7.6.2",
        "@babel/preset-env": "7.7.1",
        "@babel/preset-react": "7.7.0",
        "babel-eslint": "10.0.3",
        "babel-loader": "8.0.6",
        "copy-webpack-plugin": "5.0.5",
        "cross-env": "6.0.3",
        "css-loader": "3.2.0",
        "ejs": "2.7.4",
        "eslint": "6.6.0",
        "eslint-config-prettier": "6.7.0",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-import-resolver-webpack": "0.11.1",
        "eslint-loader": "3.0.2",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-react": "7.16.0",
        "eslint-plugin-prettier": "3.1.1",
        "fibers": "4.0.2",
        "file-loader": "4.2.0",
        "gh-pages": "2.1.1",
        "husky": "3.1.0",
        "jsdoc": "3.6.3",
        "mini-css-extract-plugin": "0.8.0",
        "prettier": "1.19.1",
        "pretty-quick": "2.0.1",
        "react-hot-loader": "4.12.18",
        "sass": "1.23.6",
        "sass-loader": "8.0.0",
        "webpack": "4.41.2",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.9.0",
        "webpack-extension-reloader": "1.1.4"
    },
    "dependencies": {
        "@babel/polyfill": "7.7.0",
        "@babel/runtime": "7.7.2",
        "@material-ui/core": "4.6.1",
        "@material-ui/icons": "4.5.1",
        "react": "16.12.0",
        "react-dom": "16.12.0"
    }
}
