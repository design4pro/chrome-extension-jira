{
    "name": "chrome-extension-jira-worklogs-tracker",
    "version": "1.0.0",
    "description": "Chrome extension for worklogs in Jira",
    "homepage": "https://github.com/design4pro/chrome-extension-jira-worklogs-tracker",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --progress --color",
        "build:dev": "cross-env NODE_ENV=development webpack --progress --color",
        "build:zip": "node tools/build-zip.js",
        "watch": "npm run build -- --watch",
        "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
        "prettier": "prettier \"src/**/*.{ts,tsx}\"",
        "prettier:write": "npm run prettier -- --write",
        "lint": "eslint --ext .ts,.tsx src",
        "lint:fix": "npm run lint -- --fix",
        "test": "yarn build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.(ts|tsx)": [
            "npm run --silent lint:fix",
            "git add"
        ]
    },
    "keywords": [
        "chrome",
        "extension",
        "jira",
        "work",
        "log"
    ],
    "author": {
        "name": "DESIGN4PRO",
        "email": "dev@design4.pro",
        "url": "https://www.design4.pro"
    },
    "license": "MIT",
    "devDependencies": {
        "@angular-devkit/build-optimizer": "0.1101.1",
        "@babel/core": "7.12.10",
        "@babel/plugin-proposal-class-properties": "7.12.1",
        "@babel/plugin-proposal-decorators": "7.12.12",
        "@babel/plugin-transform-regenerator": "7.12.1",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-react": "7.12.10",
        "@babel/preset-typescript": "7.12.7",
        "@types/chrome": "^0.0.129",
        "@types/jest": "26.0.20",
        "@types/node": "14.14.22",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@typescript-eslint/eslint-plugin": "4.14.1",
        "@typescript-eslint/parser": "4.14.1",
        "babel-loader": "8.2.2",
        "babel-plugin-macros": "3.0.1",
        "babel-plugin-transform-async-to-promises": "0.8.15",
        "browserslist": "4.16.1",
        "copy-webpack-plugin": "7.0.0",
        "core-js": "3.8.3",
        "cross-env": "7.0.3",
        "ejs": "3.1.5",
        "eslint": "7.18.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-import-resolver-webpack": "0.13.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.3.1",
        "eslint-plugin-react": "7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "2.4.3",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "6.1.0",
        "husky": "4.3.8",
        "lint-staged": "10.5.3",
        "mini-css-extract-plugin": "1.3.4",
        "prettier": "2.2.1",
        "react-hooks": "^1.0.1",
        "react-hot-loader": "4.13.0",
        "source-map-loader": "2.0.0",
        "terser": "5.5.1",
        "terser-webpack-plugin": "5.1.1",
        "ts-loader": "8.0.14",
        "tsconfig-paths-webpack-plugin": "3.3.0",
        "typescript": "4.1.3",
